{
  "name": "pdir/file-fill-bundle",
  "description": "File fill bundle for Contao CMS",
  "type": "contao-bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "pdir GmbH",
      "homepage": "https://pdir.de"
    }
  ],
  "require": {
    "php": "^7.2",
    "contao/core-bundle": "^4.9",
    "symfony/http-kernel": "~2.7 || ^3.0",
    "symfony/dependency-injection": "~2.7 || ^3.0",
    "symfony/config": "~2.7 || ^3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.2",
    "phpunit/phpunit": "^7.5",
    "symfony/phpunit-bridge": "^4.3",
    "php-http/guzzle6-adapter": "^2.0"
  },
  "extra": {
    "contao-manager-plugin": "Pdir\\FileFillBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Pdir\\FileFillBundle\\": "src"
    }
  },
  "support": {
    "docs": "https://pdir.de/docs/de/contao/extensions/file-fill/",
    "issues": "https://github.com/pdir/file-fill-bundle/issues",
    "source": "https://github.com/pdir/file-fill-bundle"
  }
}
