{
  "name": "pdir/contao-file-fill-bundle",
  "description": "File fill bundle for Contao CMS",
  "type": "contao-bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "pdir GmbH",
      "homepage": "https://pdir.de"
    }
  ],
  "require": {
    "php": "^7.2",
    "contao/core-bundle": "^4.4"
  },
  "require-dev": {
    "contao/easy-coding-standard": "^3.0",
    "contao/test-case": "^4.0",
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.12",
    "phpunit/phpunit": "^8.4",
    "symfony/http-client": "4.4.*",
    "symfony/phpunit-bridge": "4.4.*",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/message-factory": "^1.0.2"
  },
  "extra": {
    "contao-manager-plugin": "Pdir\\FileFillBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Pdir\\FileFillBundle\\": "src"
    }
  },
  "support": {
    "docs": "https://pdir.de/docs/de/contao/extensions/file-fill/",
    "issues": "https://github.com/pdir/file-fill-bundle/issues",
    "source": "https://github.com/pdir/file-fill-bundle"
  }
}
